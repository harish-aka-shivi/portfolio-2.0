{
    "extends": "airbnb",
    "env": {
      "jest": true,
      "browser": true,
      "node": true
    },
    "plugins": ["jsx-a11y"],
    "rules": {
      "import/prefer-default-export": 0,
      "no-restricted-syntax": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}