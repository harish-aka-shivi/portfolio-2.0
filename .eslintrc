{
    "extends": "airbnb",
    "env": {
      "jest": true,
      "browser": true,
      "node": true
    },
    "plugins": [
      "jsx-a11y",
      "react-hooks"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "no-restricted-syntax": 0,
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}